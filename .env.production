# Production Environment Variables
# NODE_ENV is automatically set by Vite based on build mode
VITE_APP_TITLE="SkillMatch AI"
VITE_APP_VERSION="1.0.0"

# API Configuration - Will be set by DigitalOcean App Platform
# VITE_API_URL will be injected by the deployment process
VITE_API_VERSION=v1

# Analytics
VITE_ANALYTICS_ID=

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=false
